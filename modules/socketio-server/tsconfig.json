{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "Node",
    "outDir": "build",
    "target": "ES6",
    "lib": ["es6"],
    "allowJs": false,
    "strict": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "sourceMap": true
  },
  "exclude": [
    "node_modules"
  ]
}
